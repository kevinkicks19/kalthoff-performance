---
import Layout from '../layouts/Layout.astro';
import { readFileSync } from 'fs';
import { parseTestimonialsFromJSON, getDisplayTestimonials } from '../lib/testimonials';

interface Testimonial {
	testimonial: string;
	name: string;
	position: string;
	organization: string;
	image?: string;
}

// Load testimonials from JSON
const testimonialsFromJSON = await parseTestimonialsFromJSON();
console.log('Total testimonials loaded from JSON:', testimonialsFromJSON.length);

// Get all testimonials to display
const displayTestimonials = getDisplayTestimonials(testimonialsFromJSON);
console.log('Display testimonials count:', displayTestimonials.length);
console.log('Display testimonials:', JSON.stringify(displayTestimonials, null, 2));

// Ensure we have testimonials to display
if (!displayTestimonials.length) {
	console.error('No testimonials available to display');
}

const services = [
	{
		title: 'Performance Programming',
		description: 'Customized training programs designed to enhance athletic performance and achieve specific goals.',
		href: '/services/performance-programming'
	},
	{
		title: 'Rehabilitation Programming',
		description: 'Specialized programs focused on recovery and return to peak performance after injury.',
		href: '/services/rehab-programming'
	},
	{
		title: 'Daily Programming',
		description: 'Structured daily training plans to maintain consistency and progress in your fitness journey.',
		href: '/services/daily-programming'
	}
];

// Welcome to Astro! Wondering what to do next? Check out the Astro documentation at https://docs.astro.build
// Don't want to use any of this? Delete everything in this file, the `assets`, `components`, and `layouts` directories, and start fresh.
---

<Layout title="Kalthoff Performance - Your Premier Fitness Destination">
	<!-- Hero Section -->
	<section class="relative min-h-screen bg-black overflow-hidden flex items-center justify-center">
		<!-- Background Pattern -->
		<div class="absolute inset-0 bg-[url('data:image/svg+xml;base64,')] opacity-5"></div>
		
		<!-- Main Content Container -->
		<div class="relative w-[90%] max-w-[1200px] flex flex-col items-center justify-center gap-4 md:gap-12 py-6 md:py-16">
			<!-- Top Service Bars -->
			<div class="grid grid-cols-2 md:grid-cols-4 gap-2 md:gap-6 w-full">
				<a href="/services/performance-programming" class="bg-white/70 backdrop-blur-sm rounded-lg p-2 md:p-6 flex items-center justify-center shadow-lg hover:bg-black hover:text-white transition-all duration-300 relative z-10 min-h-[50px] md:min-h-[80px]">
					<span class="text-sm md:text-xl font-bebas tracking-wider text-center">Performance Programs</span>
				</a>
				<a href="/services/rehab-programming" class="bg-white/70 backdrop-blur-sm rounded-lg p-2 md:p-6 flex items-center justify-center shadow-lg hover:bg-black hover:text-white transition-all duration-300 relative z-10 min-h-[50px] md:min-h-[80px]">
					<span class="text-sm md:text-xl font-bebas tracking-wider text-center">Rehab Programs</span>
				</a>
				<a href="/services/daily-programming" class="bg-white/70 backdrop-blur-sm rounded-lg p-2 md:p-6 flex items-center justify-center shadow-lg hover:bg-black hover:text-white transition-all duration-300 relative z-10 min-h-[50px] md:min-h-[80px]">
					<span class="text-sm md:text-xl font-bebas tracking-wider text-center">Daily Life Programs</span>
				</a>
				<a href="/services/team-training" class="bg-white/70 backdrop-blur-sm rounded-lg p-2 md:p-6 flex items-center justify-center shadow-lg hover:bg-black hover:text-white transition-all duration-300 relative z-10 min-h-[50px] md:min-h-[80px]">
					<span class="text-sm md:text-xl font-bebas tracking-wider text-center">Team Training</span>
				</a>
			</div>

			<!-- Center Image -->
			<div class="w-full aspect-[16/9] overflow-hidden border-4 border-white shadow-lg relative z-10 my-4 md:my-8">
				<img src="/images/kalthoffPerformanceLanding.jpg" alt="Kalthoff Performance" class="w-full h-full object-cover" loading="eager" decoding="async" fetchpriority="high" />
				<div class="absolute inset-0 bg-gradient-to-t from-black/50 to-transparent"></div>
			</div>

			<!-- Bottom Service Bars -->
			<div class="grid grid-cols-2 md:grid-cols-4 gap-2 md:gap-6 w-full">
				<a href="/services/home-gym-design" class="bg-white/70 backdrop-blur-sm rounded-lg p-2 md:p-6 flex items-center justify-center shadow-lg hover:bg-black hover:text-white transition-all duration-300 relative z-10 min-h-[50px] md:min-h-[80px]">
					<span class="text-sm md:text-xl font-bebas tracking-wider text-center">Home Gym Design</span>
				</a>
				<a href="/services/mentorship" class="bg-white/70 backdrop-blur-sm rounded-lg p-2 md:p-6 flex items-center justify-center shadow-lg hover:bg-black hover:text-white transition-all duration-300 relative z-10 min-h-[50px] md:min-h-[80px]">
					<span class="text-sm md:text-xl font-bebas tracking-wider text-center">Collaboration</span>
				</a>
				<a href="/services/sport-science" class="bg-white/70 backdrop-blur-sm rounded-lg p-2 md:p-6 flex items-center justify-center shadow-lg hover:bg-black hover:text-white transition-all duration-300 relative z-10 min-h-[50px] md:min-h-[80px]">
					<span class="text-sm md:text-xl font-bebas tracking-wider text-center">Sport Science</span>
				</a>
				<a href="/about" class="bg-white/70 backdrop-blur-sm rounded-lg p-2 md:p-6 flex items-center justify-center shadow-lg hover:bg-black hover:text-white transition-all duration-300 relative z-10 min-h-[50px] md:min-h-[80px]">
					<span class="text-sm md:text-xl font-bebas tracking-wider text-center">About</span>
				</a>
			</div>
		</div>
	</section>

	<!-- About Section (from About page) -->
	<section class="py-16 bg-white">
		<div class="container mx-auto px-4">
			<div class="max-w-6xl mx-auto">
				<h2 class="text-5xl font-bebas tracking-wider mb-8 text-center">About Me</h2>
				<div class="flex flex-col md:flex-row items-center gap-12">
					<div class="md:w-1/2">
						<p class="text-xl font-inter text-gray-700 leading-relaxed">
							With over 15 years of experience in high-performance sport, I have dedicated my career to optimizing human movement, injury prevention, and overall quality of life through strength and conditioning, sport science, and rehabilitation. My journey has taken me from competing at the professional level in soccer to coaching Olympic and professional athletes in multiple sports, as well as university teams. More recently, I have expanded my focus to working with the general populationâ€”helping individuals improve their movement, prevent injuries, and enhance their overall well-being. As a Certified Strength & Conditioning Specialist (CSCS) with a Master of Science in Sport Science and Strength & Conditioning, I specialize in evidence-based training methodologies that support both athletic performance and long-term health.
						</p>
					</div>
					<div class="md:w-1/2">
						<img 
							src="/images/kalthoffAbout.jpg" 
							alt="Zach Kalthoff" 
							class="w-full h-[500px] object-cover rounded-lg shadow-lg"
						/>
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- Testimonials Section -->
	<section class="py-12 md:py-20 bg-white">
		<div class="container mx-auto px-4">
			<h2 class="text-3xl md:text-6xl font-bebas text-center mb-6 md:mb-12 tracking-wider">What Our Athletes Say</h2>
			
			<!-- Testimonial Slideshow -->
			<div class="relative max-w-4xl mx-auto px-4 sm:px-6 md:px-8" role="region" aria-label="Testimonials">
				<!-- Testimonials Container -->
				<div class="overflow-hidden relative" role="group" aria-roledescription="carousel">
					<div id="testimonialSlider" class="flex transition-transform duration-500" role="list">
						{displayTestimonials.map((testimonial, index) => (
							<div class="w-full flex-shrink-0" role="listitem" aria-roledescription="slide" aria-label={`Testimonial ${index + 1} of ${displayTestimonials.length}`}>
								<blockquote class="text-center bg-white rounded-lg p-4 md:p-8 shadow-lg flex flex-col min-h-[350px] md:min-h-[400px]">
									<!-- Profile Picture Placeholder -->
									<div class="w-24 h-24 md:w-40 md:h-40 bg-gray-200 rounded-full flex items-center justify-center mx-auto mb-4 md:mb-6">
										{testimonial.image ? (
											<img 
												src={testimonial.image} 
												alt={`${testimonial.name}'s profile picture`} 
												class="w-full h-full rounded-full object-cover"
												loading="lazy"
											/>
										) : (
											<span class="text-gray-500 text-3xl md:text-5xl" aria-hidden="true">{testimonial.name.charAt(0)}</span>
										)}
									</div>
									
									<!-- Testimonial Content -->
									<div class="flex-grow">
										<p class="text-gray-700 italic text-sm md:text-lg mb-4 md:mb-6">"{testimonial.testimonial}"</p>
										
										<!-- Name and Title -->
										<footer>
											<cite class="font-bebas text-base md:text-xl tracking-wide">{testimonial.name}</cite>
											<p class="text-gray-600 text-xs md:text-sm mt-1">
												{[testimonial.position, testimonial.organization].filter(Boolean).join(', ')}
											</p>
										</footer>
									</div>

									<!-- Navigation Elements -->
									<div class="mt-auto pt-4 md:pt-6 flex items-center justify-between">
										<button 
											class="bg-black text-white w-8 h-8 md:w-10 md:h-10 rounded-full flex items-center justify-center hover:bg-gray-800 transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-black"
											aria-label="Previous testimonial"
											tabindex="0"
										>
											<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 md:h-6 md:w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor" aria-hidden="true">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7" />
											</svg>
										</button>
										
										<div class="flex space-x-2" role="tablist" aria-label="Testimonial navigation">
											{displayTestimonials.map((_, dotIndex) => (
												<button 
													class={`w-3 h-3 rounded-full ${dotIndex === index ? 'bg-black' : 'bg-gray-300'} focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-black`} 
													data-slide-index={dotIndex}
													aria-label={`Go to testimonial ${dotIndex + 1}`}
													role="tab"
													aria-selected={dotIndex === index ? "true" : "false"}
													tabindex={dotIndex === index ? "0" : "-1"}
												></button>
											))}
										</div>

										<button 
											class="bg-black text-white w-8 h-8 md:w-10 md:h-10 rounded-full flex items-center justify-center hover:bg-gray-800 transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-black"
											aria-label="Next testimonial"
											tabindex="0"
										>
											<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 md:h-6 md:w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor" aria-hidden="true">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
											</svg>
										</button>
									</div>
								</blockquote>
							</div>
						))}
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- CNWST Slideshow Section -->
	<section class="py-8 md:py-16 bg-black">
		<div class="container mx-auto px-4">
			<div class="max-w-6xl mx-auto">
				<div class="relative rounded-lg overflow-hidden h-[300px] sm:h-[400px] md:h-[500px] lg:h-[700px]" role="region" aria-label="Canada Soccer Training Slideshow">
					<div class="flex transition-transform duration-500" id="cnwstSlider" role="group" aria-roledescription="carousel">
						<!-- Slide 1 -->
						<div class="w-full flex-shrink-0" role="listitem" aria-roledescription="slide" aria-label="Slide 1 of 9">
							<img src="/images/CNWST/kalthoffCNWST.png" alt="Canada Soccer Training" class="w-full h-full object-contain" loading="lazy" />
						</div>
						<!-- Slide 2 -->
						<div class="w-full flex-shrink-0" role="listitem" aria-roledescription="slide" aria-label="Slide 2 of 9">
							<img src="/images/CNWST/kalthoffCNWST2.png" alt="Canada Soccer Training" class="w-full h-full object-contain" loading="lazy" />
						</div>
						<!-- Slide 3 -->
						<div class="w-full flex-shrink-0" role="listitem" aria-roledescription="slide" aria-label="Slide 3 of 9">
							<img src="/images/CNWST/e7dfdb49-d2f5-4d5f-816a-ede1ae76e50e.jpg" alt="Canada Soccer Training" class="w-full h-full object-contain" loading="lazy" />
						</div>
						<!-- Slide 4 -->
						<div class="w-full flex-shrink-0" role="listitem" aria-roledescription="slide" aria-label="Slide 4 of 9">
							<img src="/images/CNWST/de5a1420-b93f-4b5c-96e1-82301542dfd0.jpg" alt="Canada Soccer Training" class="w-full h-full object-contain" loading="lazy" />
						</div>
						<!-- Slide 5 -->
						<div class="w-full flex-shrink-0" role="listitem" aria-roledescription="slide" aria-label="Slide 5 of 9">
							<img src="/images/CNWST/C98E40B7-502E-419C-88AC-607F1FDB792D.jpg" alt="Canada Soccer Training" class="w-full h-full object-contain" loading="lazy" />
						</div>
						<!-- Slide 6 -->
						<div class="w-full flex-shrink-0" role="listitem" aria-roledescription="slide" aria-label="Slide 6 of 9">
							<img src="/images/CNWST/42639d79-52d4-4ec8-86db-6e6e4ac5a52d.jpg" alt="Canada Soccer Training" class="w-full h-full object-contain" loading="lazy" />
						</div>
						<!-- Slide 7 -->
						<div class="w-full flex-shrink-0" role="listitem" aria-roledescription="slide" aria-label="Slide 7 of 9">
							<img src="/images/CNWST/ce983ba1-ac33-49ae-8b88-8467d153ac9b.jpg" alt="Canada Soccer Training" class="w-full h-full object-contain" loading="lazy" />
						</div>
						<!-- Slide 8 -->
						<div class="w-full flex-shrink-0" role="listitem" aria-roledescription="slide" aria-label="Slide 8 of 9">
							<img src="/images/CNWST/54764e65-117b-4b1c-85d5-6c3189fd825c.jpg" alt="Canada Soccer Training" class="w-full h-full object-contain" loading="lazy" />
						</div>
						<!-- Slide 9 -->
						<div class="w-full flex-shrink-0" role="listitem" aria-roledescription="slide" aria-label="Slide 9 of 9">
							<img src="/images/CNWST/e0ac98a2-3b9c-4fa9-b0af-9f3c712c320d.jpg" alt="Canada Soccer Training" class="w-full h-full object-contain" loading="lazy" />
						</div>
					</div>
					<!-- Navigation Dots -->
					<div class="absolute bottom-4 left-1/2 transform -translate-x-1/2 flex space-x-2" role="tablist" aria-label="Slideshow navigation">
						<button class="w-2 h-2 rounded-full bg-white/50 hover:bg-white transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-white" onclick="goToCNWSTSlide(0)" aria-label="Go to slide 1" role="tab" aria-selected="true" tabindex="0"></button>
						<button class="w-2 h-2 rounded-full bg-white/50 hover:bg-white transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-white" onclick="goToCNWSTSlide(1)" aria-label="Go to slide 2" role="tab" aria-selected="false" tabindex="-1"></button>
						<button class="w-2 h-2 rounded-full bg-white/50 hover:bg-white transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-white" onclick="goToCNWSTSlide(2)" aria-label="Go to slide 3" role="tab" aria-selected="false" tabindex="-1"></button>
						<button class="w-2 h-2 rounded-full bg-white/50 hover:bg-white transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-white" onclick="goToCNWSTSlide(3)" aria-label="Go to slide 4" role="tab" aria-selected="false" tabindex="-1"></button>
						<button class="w-2 h-2 rounded-full bg-white/50 hover:bg-white transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-white" onclick="goToCNWSTSlide(4)" aria-label="Go to slide 5" role="tab" aria-selected="false" tabindex="-1"></button>
						<button class="w-2 h-2 rounded-full bg-white/50 hover:bg-white transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-white" onclick="goToCNWSTSlide(5)" aria-label="Go to slide 6" role="tab" aria-selected="false" tabindex="-1"></button>
						<button class="w-2 h-2 rounded-full bg-white/50 hover:bg-white transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-white" onclick="goToCNWSTSlide(6)" aria-label="Go to slide 7" role="tab" aria-selected="false" tabindex="-1"></button>
						<button class="w-2 h-2 rounded-full bg-white/50 hover:bg-white transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-white" onclick="goToCNWSTSlide(7)" aria-label="Go to slide 8" role="tab" aria-selected="false" tabindex="-1"></button>
						<button class="w-2 h-2 rounded-full bg-white/50 hover:bg-white transition-colors focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-white" onclick="goToCNWSTSlide(8)" aria-label="Go to slide 9" role="tab" aria-selected="false" tabindex="-1"></button>
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- CTA Section -->
	<section class="py-20 bg-black text-white">
		<div class="container mx-auto px-4 text-center max-w-6xl">
			<div class="flex flex-col items-center justify-center">
			<h2 class="text-6xl font-bebas mb-8 tracking-wider">Ready to Start Your Fitness Journey?</h2>
			<p class="text-2xl mb-8 text-gray-300 font-inter font-light tracking-wide">Join us today and transform your life through fitness</p>
				<a href="/contact" class="inline-block bg-white text-black px-8 py-3 rounded-full text-lg font-bebas tracking-wider hover:bg-gray-200 transition duration-300">
				Get Started Now
			</a>
			</div>
		</div>
	</section>
</Layout>

<script>
	document.addEventListener('DOMContentLoaded', function() {
		// Get all slides, dots, and buttons
		const slider = document.getElementById('testimonialSlider');
		const slides = Array.from(document.querySelectorAll('#testimonialSlider > div'));
		let currentSlide = 0;
		let sliderInterval: number | undefined;

		// Update the active slide
		function updateSlider() {
			if (slider) {
				slider.style.transform = `translateX(-${currentSlide * 100}%)`;
			}
			
			// Update dots in all slides
			slides.forEach((slide, slideIndex) => {
				const dots = Array.from(slide.querySelectorAll('[data-slide-index]'));
				dots.forEach((dot, dotIndex) => {
					if (dotIndex === currentSlide) {
						dot.classList.remove('bg-gray-300');
						dot.classList.add('bg-black');
					} else {
						dot.classList.remove('bg-black');
						dot.classList.add('bg-gray-300');
					}
					dot.setAttribute('aria-current', dotIndex === currentSlide ? 'true' : 'false');
				});
			});
		}

		// Function to show full testimonial
		function showFullTestimonial(testimonial: { testimonial: string; name: string; position: string; organization: string; image?: string }) {
			const modal = document.createElement('div');
			modal.className = 'fixed inset-0 bg-black/80 flex items-center justify-center z-50 p-4';
			modal.innerHTML = `
				<div class="bg-white rounded-lg max-w-2xl w-full p-8 relative">
					<button class="absolute top-4 right-4 text-gray-500 hover:text-gray-700">
						<svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12" />
						</svg>
					</button>
					<div class="flex items-center mb-6">
						<img src="${testimonial.image}" alt="${testimonial.name}" class="w-24 h-24 rounded-full object-cover mr-6">
						<div>
							<h3 class="text-2xl font-bebas tracking-wide">${testimonial.name}</h3>
							<p class="text-gray-600">${[testimonial.position, testimonial.organization].filter(Boolean).join(', ')}</p>
						</div>
					</div>
					<p class="text-gray-700 text-lg">"${testimonial.testimonial}"</p>
				</div>
			`;
			
			document.body.appendChild(modal);
			
			// Close modal when clicking the close button or outside the modal
			modal.addEventListener('click', (e: MouseEvent) => {
				const target = e.target as HTMLElement;
				if (target === modal || target.closest('button')) {
					modal.remove();
				}
			});
		}

		// Add event listeners to all slides
		slides.forEach((slide, index) => {
			// Add click event to testimonial content
			slide.addEventListener('click', (e) => {
				const target = e.target as HTMLElement;
				// Only show full testimonial if clicking on the content, not the navigation
				if (!target.closest('button') && !target.closest('[data-slide-index]')) {
					// @ts-ignore - displayTestimonials is defined in the Astro component
					showFullTestimonial(displayTestimonials[index]);
				}
			});

			// Add event listeners to navigation buttons
			const prevButton = slide.querySelector('[aria-label="Previous testimonial"]');
			const nextButton = slide.querySelector('[aria-label="Next testimonial"]');
			const dots = Array.from(slide.querySelectorAll('[data-slide-index]'));

			if (prevButton) {
				prevButton.addEventListener('click', (e) => {
					e.stopPropagation(); // Prevent triggering the slide click event
					currentSlide = (currentSlide - 1 + slides.length) % slides.length;
					updateSlider();
				});
			}

			if (nextButton) {
				nextButton.addEventListener('click', (e) => {
					e.stopPropagation(); // Prevent triggering the slide click event
					currentSlide = (currentSlide + 1) % slides.length;
					updateSlider();
				});
			}

			dots.forEach((dot, dotIndex) => {
				dot.addEventListener('click', (e) => {
					e.stopPropagation(); // Prevent triggering the slide click event
					currentSlide = dotIndex;
					updateSlider();
				});
			});
		});
		
		// Start the slider with a 25-second interval
		function startSlider() {
			sliderInterval = window.setInterval(() => {
				currentSlide = (currentSlide + 1) % slides.length;
				updateSlider();
			}, 25000); // 25 seconds
		}
		
		// Initialize the slider
		updateSlider();
		startSlider();
		
		// Pause the slider when hovering over it
		slider?.addEventListener('mouseenter', () => {
			if (sliderInterval) {
				clearInterval(sliderInterval);
			}
		});
		
		// Resume the slider when mouse leaves
		slider?.addEventListener('mouseleave', () => {
			startSlider();
		});

		// CNWST Slider
		let currentCNWSTSlide = 0;
		const cnwstSlider = document.getElementById('cnwstSlider');

		function updateCNWSTSlider() {
			if (cnwstSlider) {
				cnwstSlider.style.transform = `translateX(-${currentCNWSTSlide * 100}%)`;
			}
		}

		function nextCNWSTSlide() {
			currentCNWSTSlide = (currentCNWSTSlide + 1) % 9;
			updateCNWSTSlider();
		}

		function goToCNWSTSlide(slideIndex: number) {
			currentCNWSTSlide = slideIndex;
			updateCNWSTSlider();
		}

		// Auto advance CNWST slides every 10 seconds
		setInterval(nextCNWSTSlide, 10000);
	});
</script>